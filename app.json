{
  "expo": {
    "name": "bsquared",
    "slug": "cb-bet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/backupBookIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashBackupBook.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A88F8"
    },
    "ios": {
      "associatedDomains": [
        "applinks:bsquared.world"
      ],
      "bundleIdentifier": "com.jbreite.cbbet",
      "buildNumber": "3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A88F8"
      },
      "package": "com.jbreite.cbbet",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "bsquared.world",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "36e1bba2-738f-4cf4-9682-306de4cb329b"
      }
    }
  }
}
